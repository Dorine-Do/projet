<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        
    </head>

    <body>
    {% include('layout/header.html.twig')%}
        <div class="container">
            {%for message in app.flashes('success')%}
                <div>
                    {{message}}
                </div>
            {%endfor%}
            {% block body %}

            {% endblock %}
            {% include('layout/footer.html.twig')%}
        </div>
        <div id="g_id_onload"
             data-client_id="939653659160-g2cn2cgin2ua2cgp33003np8quf1l7m1.apps.googleusercontent.com"
             data-login_uri="http://localhost:8000"
             data-auto_select="true"
{#             data-username="{{ last_username }}"#}
             data-context="signin">
        </div>
        <script src="https://accounts.google.com/gsi/client" async defer></script>
        <script src="{{ asset('js/onetap.js') }}"></script>
        <script src="{{ asset('js/disconnect.js') }}"></script>
         {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
   
</html>